int srch(int arr[], int l, int r, int x) { 
    while (l <= r) { 
        int m = l + (r-l)/2; 
        if (a[m] == x) 
            return m; 
        if (a[m] < x) 
            l = m + 1; 
        else
            r = m - 1; 
    } 
